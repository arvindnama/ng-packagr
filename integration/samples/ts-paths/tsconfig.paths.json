{
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "fullTemplateTypeCheck": true,
    "enableIvy": false
  },
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "",
    "declaration": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": ["dom", "es2015"],
    "paths": {
      "@ts-path/api": ["./src/api/index"]
    }
  },
  "exclude": ["node_modules", "dist"],
  "files": ["AUTOGENERATED"]
}
